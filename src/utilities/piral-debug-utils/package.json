{
  "name": "piral-debug-utils",
  "version": "1.8.3",
  "description": "Utilities for debugging Piral instances.",
  "keywords": [
    "piral",
    "smapiot",
    "pilet",
    "modules",
    "debug",
    "development",
    "utilities"
  ],
  "author": "smapiot",
  "homepage": "https://piral.io",
  "license": "MIT",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./lib/*": {
      "import": "./lib/*",
      "types": "./lib/*.d.ts"
    },
    "./debug": "./lib/debug.js",
    "./emulator": "./lib/emulator.js",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "lib",
    "src",
    "debug.d.ts",
    "emulator.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/smapiot/piral.git"
  },
  "bugs": {
    "url": "https://github.com/smapiot/piral/issues"
  },
  "scripts": {
    "cleanup": "rimraf esm lib",
    "build": "yarn build:esnext",
    "build:esnext": "tsc --project tsconfig.json --outDir lib --module esnext",
    "typedoc": "typedoc --json ../../../docs/types/piral-debug-utils.json src --exclude \"src/**/*.test.*\"",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "devDependencies": {
    "piral-base": "^1.8.3"
  }
}
